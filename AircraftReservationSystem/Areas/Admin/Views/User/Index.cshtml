<!-- Views/User/Index.cshtml -->
@model List<Passenger>

<h1>User Management</h1>

<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <!-- Add other columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <!-- Add other columns as needed -->
            </tr>
        }
    </tbody>
</table>
